{
  "version": 2,
  "buildCommand": "npm install",
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/env-check",
      "dest": "/api/env-check.js"
    },
    {
      "src": "/api/migrate",
      "dest": "/api/migrate.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
